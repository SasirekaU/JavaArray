package com.capgi.day2;
import java.util.Arrays;
public class DeleteElementOfArray {
public static void main(String args[])
{
	int ar[]= {1,2,3,4};
	System.out.println("original array" +Arrays.toString(ar));
	int del=1;
	for(int i=del;i<ar.length - 1;i++)
	{
		ar[i]=ar[i + 1];
	}
	System.out.println("After deleting the element" +Arrays.toString(ar));
}
}
